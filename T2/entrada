int factorial(int num){
    int i,f;
    i=1;
    f=1;
    while(i<=num){
        f=f*i;
        i=i+1;
    }
    return f;
}
{
    int b,c;
    string fac;
    fac = "fatorial de";
    read(c);
    b=factorial(c);
    print(fac);
    print(c);
    print("Ã© igual a:");
    print(b);
    if(b>=25){
        print("maior que 25");
    }
    else{
        print("menor que 25");
    }
    return;
}
